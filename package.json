  "repository": {
    "type": "git",
    "url": "https://github.com/redpulse96/droid-zero"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "commit-msg": "echo $HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "./scripts/build.sh",
    "build:binary": "./scripts/build-binary",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "dev:lint": "nodemon --config ./nodemon-lint.json",
    "deploy": "./scripts/deploy.sh",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2 start --name 'droid-zero' ./dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cli": "ts-node -r tsconfig-paths/register src/console.ts"
  },
